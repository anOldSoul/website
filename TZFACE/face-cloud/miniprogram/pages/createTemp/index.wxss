
.addPw_input{
  width: 100%;
  padding: 40rpx 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1rpx solid #efefef;
}
checkbox-group {
  width: 450rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.checkitem {
  margin-right: 20rpx;
  margin-bottom: 15rpx;
}
input {
  font-size: 30rpx;
}
picker {
  width: 90%;
}
.list_more{
  width: 14rpx;
  height: 28rpx;
}
.fixed{
  width: 100%;
  position: fixed;
  bottom: 0;
  padding-bottom: 60rpx;
  padding-top: 30rpx;
  z-index: 99;
  background: #fff;
}
.tip{
  font-size:28rpx;
  color:rgba(153,153,153,1);
  padding:0 30rpx;
}
.temp_box{
  width:690rpx;
  height:250rpx;
  background:rgba(255,255,255,1);
  box-shadow:0 2rpx 12rpx 0 rgba(0,0,0,0.1);
  border-radius:12rpx;
  margin-left: 30rpx;
  margin-bottom: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 60rpx;
}
.add_password{
  width:670rpx;
  height:94rpx;
  color: #fff;
  background:rgba(9,25,40,1);
  font-size:36rpx;
  box-shadow:0 4rpx 8rpx 0 rgba(9,25,40,0.24);
  border-radius:12rpx;
  line-height: 94rpx;
  text-align: center;
  margin-left: 40rpx;
  margin-top: 40rpx;
}
.sync_password{
  width:670rpx;
  height:94rpx;
  color:rgba(153,153,153,1);
  border:2rpx solid rgba(153,153,153,1);
  font-size:36rpx;
  border-radius:12rpx;
  line-height: 94rpx;
  margin-top: 40rpx;
}
.person {
  width: 150rpx;
  height: 150rpx;
  background: #ececec;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  position: relative;
  border-radius: 100rpx;
  margin-left: 10rpx;
  overflow: hidden;
  border: 1rpx dotted black;
}
.icon_img{
  width: 48rpx;
  height: 48rpx;
}
.face {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.face_name {
  width: 100%;
  font-size: 22rpx;
  text-align: center;
  margin-top: 7rpx;
}
.share_pw {
  width:670rpx;
  height:94rpx;
  font-size:36rpx;
  box-shadow:0 4rpx 8rpx 0 rgba(9,25,40,0.24);
  border-radius:12rpx;
  line-height: 94rpx;
  text-align: center;
  margin-left: 40rpx;
  margin-top: 40rpx;
}