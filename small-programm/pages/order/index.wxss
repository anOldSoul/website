button::after{
  border: none;
  border-radius: 0;
  background: none;
}
button{
  border: none;
  border-radius: 0;
  padding: 0;
  background: #fff;
  font-size: 26rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: var(--colorG);
}
.btnList{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  padding-right: 20rpx;
  padding-bottom: 30rpx;
  border-bottom: 20rpx solid #f6f6f6;
}
.order_goods{
  margin-left: 20rpx;
  height: 170rpx;
}
.goods_list{
  display: flex;
  flex-direction: row;
}
.item{
  margin-right: 20rpx;
  display: flex;
  flex-direction: row;
}
.item_name{
  height: 100%;
  display: flex;
  align-items: center;
