  height: 100%;
  background: #fff;
}
.order_status{
  color: var(--colorG);
  font-size: 24rpx;
}
.title{
  padding: 30rpx 20rpx;
  font-size: 26rpx;
  color: #323232;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.icon_img{
  width: 30rpx;
