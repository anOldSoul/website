}
.time{
  margin-left: 60rpx;
  font-size: 24rpx;
  color: #323232;
  opacity: 0.8;
  line-height: 1;
  margin-top: 20rpx;
}
.btn{
  font-size: 26rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  border: 1rpx solid #323232;
  opacity: 0.8;
  border-radius: 2rpx;
  margin-left: 20rpx;
  padding: 0 15rpx;
}
.btn1{
  font-size: 26rpx;
