page{
  width: 100%;
  height: 100%;
}
.empty_order{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.pic_kzt{
  width: 200rpx;
  height: 200rpx;
  margin-top: 260rpx;
}
.empty_text1{
  font-size: 36rpx;
  line-height: 1;
