.title{
  padding: 20rpx 40rpx;
  font-size: 24rpx;
  color: #323232;
  border-bottom: 1rpx solid #f4f4f4;
  border-top: 30rpx solid #f4f4f4;
}
.orderDetailList{
  margin-top: 40rpx;
}
.orderDetailItem{
  margin: 0rpx 50rpx;
  display: flex;
  flex-direction: row;
}
.item_left{
  width: 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 40rpx;
}
.item_right{
  line-height: 1;
  font-size: 30rpx;
}
.item_rightLatest{
  color: #ff4400;
  line-height: 1;
  font-size: 30rpx;
}
.item_middle{
  font-size: 20rpx;
  margin-right: 60rpx;
  color: #323232;
}
.popularityValue{
  font-size: 30rpx;
  margin-bottom:20rpx;
  line-height:1;
}
.popularityOrigin{
  font-size: 26rpx;
  margin-bottom:20rpx;
  line-height:1;
}
.createDate{
  font-size: 22rpx;
  opacity: 0.5;
}
.circle_box{
  width: 30rpx;
  height: 30rpx;
  border-radius: 15rpx;
  background: #cccccc;
}
.circle_box image{
  width: 100%;
  height: 100%;
}
.line_box{
  width: 1rpx;
  height: 90rpx;
  margin: 15rpx 0;
  border-left: 1rpx solid #cccccc;
}