page{
  width: 100%;
  height: 100%;
}
image{
  width: 100%;
  height: 100%;
}
.header{
  padding: 0 100rpx;
  background: var(--colorG);
  color: #ffffff;
  height: 240rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header_img{
  width: 195rpx;
  height: 195rpx;
}
.title{
  font-size: 30rpx;
  margin: 30rpx 0;
  display: flex;
  flex-direction: row;
  color: #A28266;
}
.title_img{
  width: 40rpx;
  height: 42rpx;
  margin-right: 20rpx;
}
.title_name{
  line-height: 1;
}
.rule_item{
  margin: 0 30rpx;
  line-height: 1.5;
  text-align: justify;
  font-size: 30rpx;
  color: #A28266;
}
.ruleItem{
  margin-bottom: 20rpx;
}
.explain{
  font-size: 24rpx;
  text-align: center;
  color: grey;
  margin-top: 50rpx;
}