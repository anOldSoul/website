page{
  height: 100%;
}
.member {
  width: 100%;
  height: 100%;
  color: #ffffff;
  background: #131929;
  padding-top: 38rpx;
}
.head {
  height: 320rpx;
  align-items: center;
  display: flex;
  flex-direction: row;
  margin-bottom: 32rpx;
  background: #2F374A;
  border-radius:64rpx 64rpx 64rpx 0;
  margin-right: 64rpx;
  
}
.head_img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 75rpx;
  overflow: hidden;
  margin-left: 64rpx;
  margin-right: 32rpx;
}
.member_name{
  font-size: 30rpx;
  margin-top: 40rpx;
  line-height: 1;
}
.item_border{
  width: 2rpx;
  height: 100rpx;
  background: #323232;
  opacity: 0.5;
}
.list_text{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.icon_img{
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}
.myList{
  display: flex;
  justify-content: space-between;
  align-items: center;
  text-align: left;
  font-size: 36rpx;
  padding-bottom: 95rpx;
}
.list{
  background: #2F374A;
  margin-right: 64rpx;
  border-radius:0 64rpx 64rpx 0;
  padding-left: 88rpx;
  padding-top: 87rpx;
  padding-right: 43rpx;
}
.list_right{
  display: flex;
  align-items: center;
  color: #cccccc;
}
.list_more{
  width: 14rpx;
  height: 28rpx;
}
.support{
  font-size: 30rpx;
  text-align: center;
  color: #cccccc;
  background: #f6f6f6;
  padding: 30rpx 0;
}